#include <iostream>

using namespace std;

int main()
{

    int x,y;
    cout<<"Please write x and y: "<<endl;
    cin>>x>>y;
    if(y==8)
    {if(x==5)
    cout<<"@@@@@"<<endl;
    else
    cout<<"#####"<<endl;}
    cout<<"$$$$$"<<endl;
    cout<<"&&&&&"<<endl;


    cout<<"============"<<endl;
    if(y==8)
    {
        if(x==5)cout<<"@@@@@"<<endl;
        else{
    cout<<"#####"<<endl;
    cout<<"$$$$$"<<endl;
    cout<<"&&&&&"<<endl;}
    }

    cout<<"============"<<endl;
    if(y==8)
    {if(x==5)
    cout<<"@@@@@"<<endl;
    else
    {cout<<"#####"<<endl;
    cout<<"$$$$$"<<endl;}
    }
    cout<<"&&&&&"<<endl;

    cout<<"============"<<endl;
     if(y==8){
    if(x==5)
    cout<<"@@@@@"<<endl;}
    else
    {cout<<"#####"<<endl;
    cout<<"$$$$$"<<endl;
    cout<<"&&&&&"<<endl;}
    return 0;
}
