#include <iostream>

using namespace std;

int main()
{
    int x,y;
    cout<<"Please write x and y: "<<endl;
    cin>>x>>y;
    if(x<10)
    if(y>10)
    cout<<"*****"<<endl;
    else
    cout<<"#####"<<endl;
    cout<<"$$$$$"<<endl;
    cout<<"============"<<endl;

    cout<<"Please write x and y: "<<endl;
    cin>>x>>y;
    if(x<10)
    {
    if(y>10)
    cout<<"*****"<<endl;
    }
    else
    {
    cout<<"#####"<<endl;
    cout<<"$$$$$"<<endl;
    }
    return 0;
}
