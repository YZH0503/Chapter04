#include <iostream>

using namespace std;

int main()
{
    int x,a,b,c,d,y;
    cin>>x;
    d=x%10;
    x=x/10;
    c=x%10;
    x=x/10;
    b=x%10;
    x=x/10;
    a=x;
    a=(a+7)%10,b=(b+7)%10;c=(c+7)%10;d=(d+7)%10;
    y=a,a=c,c=y;
    y=b,b=d,d=y;
    cout<<a<<b<<c<<d;
    return 0;
}
